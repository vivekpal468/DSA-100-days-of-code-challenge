// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int linearSearch(int arr[], int ind, int key)
{
    if(ind==-1) return 0;
    if(arr[ind] != key)
    linearSearch(arr, ind-1, key);
    else
    return 1;
}
int main() {
    // Write C++ code here
    int arr[]={5,7,6,9,2};
    cout<<linearSearch(arr, 5, 8);
    
    return 0;
}
