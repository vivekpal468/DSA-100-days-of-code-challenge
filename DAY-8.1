class Solution{
    
    public:
    int Maximum_Sum(vector<vector<int>> &mat,int n,int k){
        // Your code goes here
        int p[n+1][n+1];
        for(int i=0;i<=n;i++)
        {
            for(int j=0;j<=n;j++)
            {
                p[i][0]=0;
                p[0][j]=0;
            }
        }
        int mx=INT_MIN;
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=n;j++)
            {
                p[i][j]=mat[i-1][j-1]+p[i][j-1]+p[i-1][j]-p[i-1][j-1];
                
                if(i>=k and j>=k)
                mx=max(mx, p[i][j]- p[i][j-k] - p[i-k][j] + p[i-k][j-k]);
            }
        }
        return mx;
    }  
};

