// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void reverse(string &str, int start, int end)
{
    //base case
    if(start>=end) return ;
    swap(str[start], str[end]);
    return reverse(str, start+1, end-1);
}
int main() {
    // Write C++ code here
    string str="VIVEK";
    reverse(str, 0, str.length()-1);
    cout<<str;
    return 0;
}
